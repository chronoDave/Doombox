{
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js --ext .jsx .",
    "lint:react": "eslint --ext .js --ext .jsx packages/doombox-react",
    "lint:electron": "eslint --ext .js packages/doombox-electron",
    "start": "yarn start:react && yarn start:electron",
    "start:react": "yarn workspace @doombox/react start",
    "start:electron": "yarn workspace @doombox/electron start:dev",
    "start:production": "yarn workspace @doombox/electron start",
    "build": "yarn build:react && yarn build:electron",
    "build:react": "yarn workspace @doombox/react build",
    "build:electron": "yarn workspace @doombox/electron build",
    "test": "yarn test:electron",
    "test:electron": "yarn workspace @doombox/electron test:unit",
    "arkit": "yarn arkit:react && yarn arkit:electron",
    "arkit:react": "yarn workspace @doombox/react arkit",
    "arkit:react-render": "yarn workspace @doombox/react arkit:render",
    "arkit:react-flow": "yarn workspace @doombox/react arkit:flow",
    "arkit:react-inheritance": "yarn workspace @doombox/react arkit:inheritance",
    "arkit:electron": "yarn workspace @doombox/electron arkit",
    "version:patch": "yarn version --patch && yarn workspaces version --patch",
    "version:minor": "yarn version --minor && yarn workspaces version --minor",
    "version:major": "yarn version --major && yarn workspaces version --major"
  },
  "devDependencies": {
    "@neutrinojs/react": "^9.0.0",
    "arkit": "^1.6.2",
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^6.0.3",
    "del-cli": "^3.0.0",
    "electron": "7.1.1",
    "electron-builder": "^22.1.0",
    "electron-mocha": "^8.1.2",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mocha": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-react-i18next": "^1.1.0",
    "mocha": "^6.2.2",
    "neutrino": "^9.0.0",
    "tap": "^14.10.6",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {}
}

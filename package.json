{
  "name": "doombox",
  "version": "2.0.0",
  "description": "Music player for your unwieldy music collection",
  "repository": {
    "type": "git",
    "url": "https://github.com/chronoDave/Doombox.git"
  },
  "author": {
    "name": "Chronocide",
    "email": "davidwolters6758@gmail.com"
  },
  "private": true,
  "license": "GPL-3.0",
  "engines": {
    "node": ">=12.10.0"
  },
  "scripts": {
    "lint": "eslint src --ext .js --ext .jsx",
    "test:electron": "node ./scripts/test.js src/electron",
    "test:client": "node ./scripts/test.js src/client",
    "test:intl": "node ./scripts/test.js src/intl",
    "test:utils": "node ./scripts/test.js src/utils",
    "test": "node ./scripts/test.js src",
    "clean": "node scripts/clean.js",
    "start:webpack": "webpack -w",
    "start:webpack-dev": "webpack -w --env devtools",
    "start:electron": "nodemon --config .nodemon.json",
    "build:webpack": "webpack --mode production",
    "build:electron": "npm run clean && electron-builder -p=never -c electron.config.js",
    "build": "npm run build:webpack && npm run build:electron"
  },
  "dependencies": {
    "@babel/preset-env": "^7.17.12",
    "@chronocide/fs-walk": "1.0.0",
    "@popperjs/core": "^2.5.3",
    "howler": "^2.2.0",
    "leaf-db": "3.5.2",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.groupby": "^4.6.0",
    "lodash.set": "^4.3.2",
    "mapsort": "^1.0.3",
    "mousetrap": "^1.6.5",
    "music-metadata": "^7.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "reselect": "^4.0.0",
    "sharp": "^0.30.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",

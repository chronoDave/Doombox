{
  "name": "doombox",
  "version": "2.0.0",
  "description": "Music player for your unwieldy music collection",
  "repository": {
    "type": "git",
    "url": "https://github.com/chronoDave/Doombox.git"
  },
  "author": "Chronocide",
  "private": true,
  "license": "GPL-3.0",
  "engines": {
    "node": ">=12.10.0"
  },
  "scripts": {
    "lint": "eslint src test",
    "start:webpack": "webpack --mode development -w",
    "start:electron": "nodemon --config .nodemon.json",
    "build:test": "node test/build.js",
    "build:webpack": "webpack --mode production",
    "build:electron": "electron-builder -p=never -c .electron.config.js",
    "build": "npm run build:webpack && npm run build:electron",
    "test:app": "npm run build:test && tape test/build/app/**/*.spec.js",
    "test:utils": "npm run build:test && tape test/build/utils/**/*.spec.js",
    "test": "npm run build:test && tape test/build/**/*.spec.js"
  },
  "devDependencies": {
    "@types/tape": "^4.13.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "chokidar": "^3.5.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "electron": "^19.0.10",
    "electron-builder": "^23.1.0",
    "esbuild": "^0.14.51",
    "esbuild-loader": "^2.19.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^2.0.19",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.2",
    "tape": "^5.5.3",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@chronocide/fs-walk": "^1.2.3",
    "forgo": "^3.2.0",
    "immer": "^9.0.15"
  }
}
